perf-util-y += util/
perf-test-y += tests/

ifdef SHELLCHECK
  SHELL_TEST_LOGS := $(SHELL_TESTS:%=%.shellcheck_log)
else
  SHELL_TESTS :=
  SHELL_TEST_LOGS :=
endif

$(OUTPUT)%.shellcheck_log: %
	$(call rule_mkdir)
	$(Q)$(call echo-cmd,test)shellcheck -a -S warning "$<" > $@ || (cat $@ && rm $@ && false)

perf-test-y += $(SHELL_TEST_LOGS)
