[Unit]
Description=Apply cpupower configuration
ConditionVirtualization=!container

[Service]
Type=oneshot
EnvironmentFile=-___CDIR___cpupower-service.conf
ExecStart=___LDIR___/cpupower
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
